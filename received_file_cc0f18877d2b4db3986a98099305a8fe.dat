#include <stdio.h>
#include <omp.h>
int main() {

    #pragma omp parallel
    printf("Hello World from thread %d/%d\n",
    omp_get_thread_num(),omp_get_num_threads());
    
    if (omp_get_thread_num()==0)
        printf("I am the first thread!!\n");
        
    printf("Goodbye World\n");

    return 0;


}